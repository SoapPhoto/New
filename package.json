{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "storybook": "start-storybook -p 9009 -s public",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "start:prod": "yarn run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "cleanAndSetup": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/clean.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js"
  },
  "dependencies": {
    "@apollo/client": "^3.3.4",
    "@craco/craco": "^6.0.0",
    "apollo-link-batch-http": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "axios": "^0.21.0",
    "chalk": "4.1.0",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "fontfaceobserver": "2.1.0",
    "formik": "^2.2.6",
    "framer-motion": "^2.9.5",
    "graphql": "^15.4.0",
    "history": "5.0.0",
    "husky": "4.3.5",
    "i18next": "19.8.4",
    "i18next-browser-languagedetector": "6.0.1",
    "i18next-scanner": "3.0.0",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.5.3",
    "lodash": "^4.17.20",
    "mobx": "^6.0.4",
    "mobx-react": "^7.0.5",
    "node-plop": "0.26.2",
    "plop": "2.7.4",
    "polished": "^4.0.5",
    "prettier": "2.2.1",
    "rc-animate": "^3.1.1",
    "rc-queue-anim": "^1.8.5",
    "react": "17.0.1",
    "react-app-polyfill": "2.0.0",
    "react-dom": "17.0.1",
    "react-feather": "^2.0.9",
    "react-helmet-async": "1.0.7",
    "react-i18next": "11.8.2",
    "react-router-dom": "6.0.0-beta.0",
    "react-scripts": "4.0.1",
    "react-spring": "^8.0.27",
    "react-test-renderer": "17.0.1",
    "replace-in-file": "6.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "3.0.2",
    "sanitize.css": "12.0.1",
    "serve": "11.3.2",
    "shelljs": "0.8.4",
    "styled-components": "5.2.1",
    "stylelint": "13.8.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-enum-util": "^4.0.2",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "web-vitals": "1.0.1",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "26.0.16",
    "@types/node": "14.14.10",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@types/react-test-renderer": "17.0.0",
    "@types/rimraf": "3.0.0",
    "@types/shelljs": "0.8.8",
    "@types/styled-components": "5.1.4",
    "@types/testing-library__jest-dom": "5.9.5",
    "@types/webpack": "4.41.25",
    "@types/webpack-env": "1.15.2",
    "@types/yup": "^0.29.10",
    "customize-cra": "^1.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "react-app-rewired": "^2.1.8",
    "terser-webpack-plugin": "^5.0.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky(remove-everything-in-these-parentheses.See-the-issue-#29)": {
    "hooks": {
      "pre-commit": "yarn run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
