{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build:vite": "tsc && vite build",
    "serve": "vite preview",
    "storybook": "start-storybook -p 9009 -s public",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "start:prod": "yarn run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "codegen-generate": "graphql-codegen",
    "cleanAndSetup": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/clean.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js"
  },
  "dependencies": {
    "@apollo/client": "^3.4.9",
    "@craco/craco": "^6.2.0",
    "@loadable/component": "5.15.0",
    "@mapbox/mapbox-gl-language": "^0.10.1",
    "@popperjs/core": "^2.9.3",
    "@types/bytes": "^3.1.1",
    "ali-oss": "^6.16.0",
    "apollo-link-batch-http": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "axios": "^0.21.1",
    "blurhash": "^1.1.4",
    "buffer": "6.0.3",
    "bytes": "^3.1.0",
    "chalk": "4.1.2",
    "cross-env": "7.0.3",
    "dayjs": "^1.10.6",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "exifr": "^7.1.3",
    "fast-average-color": "^6.4.1",
    "fontfaceobserver": "2.1.0",
    "formik": "^2.2.9",
    "gcoord": "^0.3.2",
    "graphql": "^15.5.1",
    "history": "5.0.1",
    "husky": "6.0.0",
    "i18next": "20.4.0",
    "i18next-browser-languagedetector": "6.1.2",
    "i18next-scanner": "3.0.0",
    "jest-styled-components": "7.0.5",
    "lint-staged": "11.1.2",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.2.0",
    "mobx": "^6.1.8",
    "mobx-react": "^7.2.0",
    "node-plop": "0.26.2",
    "overlayscrollbars": "^1.13.1",
    "overlayscrollbars-react": "^0.2.3",
    "path-complete-extname": "1.0.0",
    "plop": "2.7.4",
    "polished": "^4.1.3",
    "prettier": "2.3.2",
    "qs": "^6.10.1",
    "rc-animate": "^3.1.1",
    "rc-queue-anim": "^2.0.0",
    "rc-util": "^5.13.2",
    "react": "17.0.2",
    "react-app-polyfill": "2.0.0",
    "react-dom": "17.0.2",
    "react-emoji-render": "^1.2.4",
    "react-feather": "^2.0.9",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "1.1.2",
    "react-hot-toast": "^2.1.1",
    "react-i18next": "11.11.4",
    "react-lazyload": "^3.2.0",
    "react-popper": "^2.2.5",
    "react-router-dom": "6.0.0-beta.2",
    "react-scripts": "^4.0.3",
    "react-spring": "^9.2.4",
    "react-switch": "^6.0.0",
    "react-test-renderer": "17.0.2",
    "react-textarea-autosize": "^8.3.3",
    "react-use": "^17.2.4",
    "react-use-gesture": "^9.1.3",
    "replace-in-file": "6.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "3.0.2",
    "sanitize.css": "12.0.1",
    "serve": "11.3.2",
    "shelljs": "0.8.4",
    "styled-components": "5.3.1",
    "styled-media-query": "^2.1.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-enum-util": "^4.0.2",
    "ts-node": "10.2.1",
    "typescript": "4.3.5",
    "web-vitals": "1.1.1",
    "webpack": "^4.44.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/typescript-operations": "^2.1.2",
    "@graphql-codegen/typescript-react-apollo": "^3.1.2",
    "@rollup/plugin-buble": "0.21.3",
    "@rollup/plugin-graphql": "1.0.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/fontfaceobserver": "2.1.0",
    "@types/jest": "27.0.1",
    "@types/loadable__component": "5.13.4",
    "@types/mapbox-gl": "^2.4.0",
    "@types/node": "16.7.2",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/react-lazyload": "^3.1.1",
    "@types/react-router-dom": "5.1.8",
    "@types/react-test-renderer": "17.0.1",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.9",
    "@types/styled-components": "5.1.13",
    "@types/testing-library__jest-dom": "5.14.1",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.16.2",
    "@types/yup": "^0.29.13",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "Buffer": "0.0.0",
    "customize-cra": "^1.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "react-app-rewired": "^2.1.8",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-polyfill-node": "0.7.0",
    "terser-webpack-plugin": "^5.1.4",
    "vite": "2.5.1",
    "vite-plugin-imp": "2.0.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky(remove-everything-in-these-parentheses.See-the-issue-#29)": {
    "hooks": {
      "pre-commit": "yarn run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
